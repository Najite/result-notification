{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/notify-results",
      "dest": "/server/api/notify-results.js"
    },
    {
      "src": "/api/test-sms",
      "dest": "/server/api/test-sms.js"
    },
    {
      "src": "/api/health",
      "dest": "/server/api/health.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
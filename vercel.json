{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
 
  "routes": [
    {
      "src": "/api/notify-result",
      "dest": "/api/notify-results.js"
    },
    
    {
      "src": "/api/test-sms",
      "dest": "/api/test-sms.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
